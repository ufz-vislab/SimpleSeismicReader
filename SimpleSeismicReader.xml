<ServerManagerConfiguration>
	<ProxyGroup name="sources">
		<!-- ================================================================== -->
		<SourceProxy name="SimpleSeismicReader" class="SimpleSeismicReader" label="SimpleSeismicReader">
			<Documentation
				 long_help="Read a 3d seismic file."
				 short_help="Read a 3d seismic file.">
			</Documentation>
			<StringVectorProperty
						name="FileName"
						animateable="0"
						command="SetFileName"
						number_of_elements="1">
				<FileListDomain name="files"/>
				<Documentation>
					This property specifies the file name for the seismic reader.
				</Documentation>
			</StringVectorProperty>

			<Hints>
				<ReaderFactory extensions="seismic"
											 file_description="Simple 3d seismic file from OpendTect" />
		 </Hints>
		</SourceProxy>
		<!-- End MyPNGReader -->
	</ProxyGroup>
	<!-- End Filters Group -->
</ServerManagerConfiguration>
